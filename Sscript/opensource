local function PPZCD_fake_script() -- EnableAutoPrediction.LocalScript 
	local script = Instance.new('LocalScript', EnableAutoPrediction)

	local EnableAutoPrediction = false
	
	script.Parent.MouseButton1Down:Connect(function()
		if EnableAutoPrediction == false then
			EnableAutoPrediction = true
		else
			EnableAutoPrediction = false
		end
	
		if EnableAutoPrediction == true then
			game:GetService('RunService'):BindToRenderStep('AutoPrediction', 0, function()
				local pingvalue = game:GetService("Stats").Network.ServerStatsItem["Data Ping"]:GetValueString()
				local split = string.split(pingvalue, " ")
				local ping = split[1]
				local pingvalue = game:GetService("Stats").Network.ServerStatsItem["Data Ping"]:GetValueString()
				local split = string.split(pingvalue, " ")
				local ping = split[1]
				if tonumber(ping) <= 10 then
					PredictionVelocity = 0.1099
				elseif tonumber(ping) < 35 then
					PredictionVelocity = 0.2294
				elseif tonumber(ping) < 40 then
					PredictionVelocity = 0.1195
				elseif tonumber(ping) < 45 then
					PredictionVelocity = 0.1207
				elseif tonumber(ping) < 50 then
					PredictionVelocity = 0.1219
				elseif tonumber(ping) < 55 then
					PredictionVelocity = 0.1238
				elseif tonumber(ping) < 60 then
					PredictionVelocity = 0.1237 
				elseif tonumber(ping) < 65 then 
					PredictionVelocity = 0.1264 
				elseif tonumber(ping) < 70 then
					PredictionVelocity = 0.1291
				elseif tonumber(ping) < 75 then
					PredictionVelocity = 0.1314
				elseif tonumber(ping) < 80 then 
					PredictionVelocity = 0.1347 
				elseif tonumber(ping) < 85 then
					PredictionVelocity = 0.1343
				elseif tonumber(ping) < 90 then
					PredictionVelocity = 0.1359
				elseif tonumber(ping) < 95 then
					PredictionVelocity = 0.1363
				elseif tonumber(ping) < 100 then
					PredictionVelocity = 0.1388
				elseif tonumber(ping) < 105 then
					PredictionVelocity = 0.1428
				elseif tonumber(ping) < 110 then
					PredictionVelocity = 0.1469
				elseif tonumber(ping) < 120 then
					PredictionVelocity = 0.14783943
				elseif tonumber(ping) < 130 then
					PredictionVelocity = 0.15275864
				elseif tonumber(ping) < 140 then
					PredictionVelocity = 0.15482643
				elseif tonumber(ping) < 150 then
					PredictionVelocity = 0.15973582
				end
			end)
			script.Parent.Parent.EnableAutoPrediction.BackgroundColor3 = Color3.fromRGB(103, 103, 154)
		end
	
		if EnableAutoPrediction == false then
			game:GetService('RunService'):UnbindFromRenderStep('AutoPrediction')
			script.Parent.Parent.EnableAutoPrediction.BackgroundColor3 = Color3.fromRGB(15, 15, 15)
		end
	end)
end
