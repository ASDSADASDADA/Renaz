coroutine.wrap(TQOFQES_fake_script)()
local function PELYTGR_fake_script() -- EnableCFrame.LocalScript 
	local script = Instance.new('LocalScript', EnableCFrame)

	local EnableCFrame = false
	local CFrameToggle = false
	
	script.Parent.MouseButton1Down:Connect(function()
		if EnableCFrame == false then
			EnableCFrame = true
		else
			EnableCFrame = false
		end
	
		if EnableCFrame == true then
			
			game:GetService("Players").LocalPlayer:GetMouse().KeyDown:Connect(function(Key)
				if Key == script.Parent.Parent.CFrameBox.Text then
					CFrameToggle = not CFrameToggle
					if CFrameToggle then
						game:GetService('RunService'):BindToRenderStep("CFrame", 0 , function()
							game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame + game.Players.LocalPlayer.Character.Humanoid.MoveDirection * script.Parent.Parent.CFrameAmount.Text
						end)
					else
						EnableCFrame = false
						game:GetService('RunService'):UnbindFromRenderStep("CFrame")
					end
				end
			end)
			script.Parent.Parent.EnableCFrame.BackgroundColor3 = Color3.fromRGB(103, 103, 154)
		end
	
		if EnableCFrame == false then
			EnableCFrame = false
			game:GetService('RunService'):UnbindFromRenderStep("CFrame")
			script.Parent.Parent.EnableCFrame.BackgroundColor3 = Color3.fromRGB(15, 15, 15)
		end
	end)
end
coroutine.wrap(PELYTGR_fake_script)()
