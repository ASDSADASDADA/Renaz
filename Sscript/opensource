local function YTQFJU_fake_script() -- EnableUnderground.LocalScript 
	local script = Instance.new('LocalScript', EnableUnderground)

	local EnableUnderground = false
	local AntiLock = false
	
	game:GetService("RunService").Heartbeat:Connect(function()
		if AntiLock then
			local Underground = game.Players.LocalPlayer.Character.HumanoidRootPart.Velocity
			game.Players.LocalPlayer.Character.HumanoidRootPart.Velocity = Vector3.new(0, -500, 0)
			game:GetService("RunService").RenderStepped:Wait()
			game.Players.LocalPlayer.Character.HumanoidRootPart.Velocity = Underground
		end
	end)
	
	script.Parent.MouseButton1Down:Connect(function()
		if EnableUnderground == false then
			EnableUnderground = true
		else
			EnableUnderground = false
		end
	
		if EnableUnderground == true then
			AntiLock = true
			script.Parent.Parent.EnableUnderground.BackgroundColor3 = Color3.fromRGB(103, 103, 154)
		end
	
		if EnableUnderground == false then
			AntiLock = false
			script.Parent.Parent.EnableUnderground.BackgroundColor3 = Color3.fromRGB(15, 15, 15)
		end
	end)
end
coroutine.wrap(YTQFJU_fake_script)()
