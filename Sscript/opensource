local function ZSDH_fake_script() -- EnableResolver1.LocalScript 
	local script = Instance.new('LocalScript', EnableResolver1)

	local EnableResolver1 = false
	
	script.Parent.MouseButton1Down:Connect(function()
		if EnableResolver1 == false then
			EnableResolver1 = true
		else
			EnableResolver1 = false
		end
	
		if EnableResolver1 == true then
			game:GetService('RunService'):BindToRenderStep('Resolver1', 0, function()
				for i,v in pairs(game.Players:GetChildren()) do
					if v.Name ~= game.Players.LocalPlayer.Name then
						local hrp = v.Character.HumanoidRootPart
						hrp.Velocity = Vector3.new(0, 0, 0)    
						hrp.AssemblyLinearVelocity = Vector3.new(0, 0, 0)   
					end
				end
			end)
			script.Parent.Parent.EnableResolver1.BackgroundColor3 = Color3.fromRGB(103, 103, 154)
		end
	
		if EnableResolver1 == false then
			game:GetService('RunService'):UnbindFromRenderStep('Resolver1')
			script.Parent.Parent.EnableResolver1.BackgroundColor3 = Color3.fromRGB(15, 15, 15)
		end
	end)
end
coroutine.wrap(ZSDH_fake_script)()
local function KQJQ_fake_script() -- EnableResolver2.LocalScript 
	local script = Instance.new('LocalScript', EnableResolver2)

	local EnableResolver2 = false
	
	script.Parent.MouseButton1Down:Connect(function()
		if EnableResolver2 == false then
			EnableResolver2 = true
		else
			EnableResolver2 = false
		end
	
		if EnableResolver2 == true then
			game:GetService('RunService'):BindToRenderStep('Resolver2', 0, function()
				for i,v in pairs(game.Players:GetChildren()) do
					if v.Name ~= game.Players.LocalPlayer.Name then
						local hrp = v.Character.HumanoidRootPart
						hrp.Velocity = v.Character.Humanoid.MoveDirection * 16.15
						hrp.AssemblyLinearVelocity = v.Character.Humanoid.MoveDirection * 16.15
					end
				end
			end)
			script.Parent.Parent.EnableResolver2.BackgroundColor3 = Color3.fromRGB(103, 103, 154)
		end
	
		if EnableResolver2 == false then
			game:GetService('RunService'):UnbindFromRenderStep('Resolver2')
			script.Parent.Parent.EnableResolver2.BackgroundColor3 = Color3.fromRGB(15, 15, 15)
		end
	end)
end
coroutine.wrap(KQJQ_fake_script)()
